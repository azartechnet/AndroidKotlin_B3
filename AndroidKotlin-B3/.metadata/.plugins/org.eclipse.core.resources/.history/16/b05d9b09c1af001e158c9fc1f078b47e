//Operators
fun main(args:Array<String>)
{
	var a=10;
	var b=20;
	print("Addition::"+(a+b))
	print("Sub::"+(a-b))
	print("Multiple::"+(a*b))
	print("Division::"+(a/b))
	print("Module::"+(a%b))
}